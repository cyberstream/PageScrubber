<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" id="https://addons.opera.com/addons/extensions/details/pagescrubber/" version="1.1"> 
    <name>PageScrubber</name>
    <description>Block online profanity, obscenities, cussing, swearing, and bad language</description>
    <author href="http://www.cyberstream.us" email="dev@cyberstream.us">cyberstream</author>
    <icon src="icon-64.png" />
    
    <preference name="replacement" value="#####" />
    <preference name="block_level" value="0" />
    <preference name="block_pages" value="false" />
    <preference name="whitelist" value="[]" />
    <preference name="blacklist" value="[]" />
    <preference name="disabled_sites" value="[]" />
</widget>